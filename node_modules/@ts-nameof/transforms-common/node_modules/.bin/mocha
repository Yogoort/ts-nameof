#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/mocha/8.0.1/node_modules/mocha/bin/mocha/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/mocha/8.0.1/node_modules/mocha/bin/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/mocha/8.0.1/node_modules/mocha/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/mocha/8.0.1/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/mocha/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules:/mnt/g/GIT/js/ts-nameof/common/node_modules:/mnt/g/GIT/js/ts-nameof/node_modules:/mnt/g/GIT/js/node_modules:/mnt/g/GIT/node_modules:/mnt/g/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules/mocha/bin/mocha/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules/mocha/bin/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules/mocha/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules:/mnt/g/GIT/js/ts-nameof/packages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/mocha" "$@"
else 
  exec node  "$basedir/../mocha/bin/mocha" "$@"
fi
