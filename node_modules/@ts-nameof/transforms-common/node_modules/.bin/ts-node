#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/ts-node/8.10.2/typescript@3.9.7/node_modules/ts-node/dist/bin.js/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/ts-node/8.10.2/typescript@3.9.7/node_modules/ts-node/dist/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/ts-node/8.10.2/typescript@3.9.7/node_modules/ts-node/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/ts-node/8.10.2/typescript@3.9.7/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/ts-node/8.10.2/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/ts-node/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules:/mnt/g/GIT/js/ts-nameof/common/node_modules:/mnt/g/GIT/js/ts-nameof/node_modules:/mnt/g/GIT/js/node_modules:/mnt/g/GIT/node_modules:/mnt/g/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules/ts-node/dist/bin.js/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules/ts-node/dist/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules/ts-node/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-common/node_modules:/mnt/g/GIT/js/ts-nameof/packages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else 
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
