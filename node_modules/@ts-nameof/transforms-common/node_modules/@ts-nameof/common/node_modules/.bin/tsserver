#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/typescript/3.9.7/node_modules/typescript/bin/tsserver/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/typescript/3.9.7/node_modules/typescript/bin/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/typescript/3.9.7/node_modules/typescript/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/typescript/3.9.7/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/typescript/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules:/mnt/g/GIT/js/ts-nameof/common/node_modules:/mnt/g/GIT/js/ts-nameof/node_modules:/mnt/g/GIT/js/node_modules:/mnt/g/GIT/node_modules:/mnt/g/node_modules:/mnt/g/GIT/js/ts-nameof/packages/common/node_modules/typescript/bin/tsserver/node_modules:/mnt/g/GIT/js/ts-nameof/packages/common/node_modules/typescript/bin/node_modules:/mnt/g/GIT/js/ts-nameof/packages/common/node_modules/typescript/node_modules:/mnt/g/GIT/js/ts-nameof/packages/common/node_modules:/mnt/g/GIT/js/ts-nameof/packages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
