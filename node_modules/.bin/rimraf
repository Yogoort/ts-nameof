#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/rimraf/3.0.2/node_modules/rimraf/bin.js/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/rimraf/3.0.2/node_modules/rimraf/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/rimraf/3.0.2/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/rimraf/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules/.registry.npmjs.org/node_modules:/mnt/g/GIT/js/ts-nameof/common/temp/node_modules:/mnt/g/GIT/js/ts-nameof/common/node_modules:/mnt/g/GIT/js/ts-nameof/node_modules:/mnt/g/GIT/js/node_modules:/mnt/g/GIT/node_modules:/mnt/g/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-babel/node_modules/rimraf/bin.js/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-babel/node_modules/rimraf/node_modules:/mnt/g/GIT/js/ts-nameof/packages/transforms-babel/node_modules:/mnt/g/GIT/js/ts-nameof/packages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
